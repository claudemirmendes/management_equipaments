<!DOCTYPE html>
<html>
<head>

  <title>Management Equipaments</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
<% if notice %>
  <p class="alert alert-success"><%= notice %></p>
<% end %>
<% if alert %>
  <p class="alert alert-danger"><%= alert %></p>
<% end %>
</head>
<nav class="navbar navbar-inverse">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="#">Gerenciamento de Equipamentos</a>
    </div>
    <ul class="nav navbar-nav">
      <li class="active"><a href="<%=home_index_path%>">Home</a></li>
      <li><a href="<%= equipment_index_path%>">Cadastro de equipamentos</a></li>
      <li><a href="<%= places_path %>">Locais</a></li>
    </ul>
  </div>
</nav>
<body>
<%= yield %>

</body>
</html>
